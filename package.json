{
  "name": "vacant",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://amitkparekh@gitlab.com/orderlychaos/elves/vacant.git",
  "author": "Amit Parekh <amit@dresser.app>",
  "license": "MIT",
  "scripts": {
    "start": "node node_modules/ts-node/dist/bin ./index.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "echo $HUSKY_GIT_STDIN | bin/pre-push-lfs.sh $HUSKY_GIT_PARAMS",
      "post-merge": "install-deps-postmerge"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,yaml,scss,sass,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@types/prompts": "^1.2.0",
    "@types/puppeteer": "^1.12.1",
    "prompts": "^2.0.2",
    "puppeteer": "^1.12.2",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^10.12.24",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.13.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4"
  }
}
